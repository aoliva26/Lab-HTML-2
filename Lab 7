        <script>
            let num1 = 10
            let num2 = 2
            
            console.log(num1 + num2)
            console.log(num1 - num2)
            console.log(num1 * num2)
            
            var score = 95;

            function calculateGrade(score) {
            if (score >= 90) {
                console.log("A grade");
            } else if (score >= 80 && score <= 89) {
                console.log("B grade");
            } else if (score >= 70 && score <= 79) {
                console.log("C grade");
            } else if (score >= 60 && score <= 69) {
                console.log("D grade");
            } else {
                console.log("F grade");
            }
            }

            calculateGrade(score);
            
       </script>

        <button id="changeTextBtn">Change Text</button>
        <p id="displayText">Original Text</p>

        <script>
            var changeTextButton = document.getElementById('changeTextBtn');
            var displayTextParagraph = document.getElementById('displayText');
            changeTextButton.addEventListener('click', function() {
            function changeText() {
            displayTextParagraph.textContent = 'Hello, JavaScript World!';
            }
            changeText();
        });
